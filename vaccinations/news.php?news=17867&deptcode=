         <script>
         function openNav() {
           document.getElementById("mySidebar").style.width = "100%";
           document.getElementById("main").style.marginLeft = "100%";
         }

         function closeNav() {
           document.getElementById("mySidebar").style.width = "0";
           document.getElementById("main").style.marginLeft= "0";
         }
         </script>

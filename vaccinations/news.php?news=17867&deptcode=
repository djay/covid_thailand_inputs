<script type="text/javascript"  >
     var facebookShare = document.querySelector('[data-js="facebook-share"]');
     var radiolive = 'https://ddc.moph.go.th/dcd/news.php?news=17867&deptcode=dcd';
    facebookShare.onclick = function(e) {
      e.preventDefault();
      var facebookWindow = window.open('https://www.facebook.com/sharer/sharer.php?u=' +radiolive, 'facebook-popup', 'height=350,width=600');
      if(facebookWindow.focus) { facebookWindow.focus(); }
        return false;
    }

    var twitterShare = document.querySelector('[data-js="twitter-share"]'); 

    var tw = 'https://twitter.com/intent/tweet?text=รายงานความก้าวหน้าการให้บริการฉีดวัคซีนโควิด 19 ประจำวันอังคารที่ 30 มีนาคม 2564&url=https://ddc.moph.go.th/dcd/news.php?news=17867&deptcode=dcd&via=DDC_info' ;

  twitterShare.onclick = function(e) {
    e.preventDefault(); 
    var twitterWindow = window.open(tw, 'twitter-popup', 'height=350,width=600');
    if(twitterWindow.focus) { twitterWindow.focus(); }
      return false;
    }

 

    </script>


